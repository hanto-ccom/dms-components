{
  "name": "dms-workspace",
  "version": "0.0.0",
  "scripts": {
    "create:bundle-entry": "echo @import \"./vendor-digi.css\"; > projects/dms/src/styles/vendor/vendor.css && echo @import \"./vendor-fonts.css\"; >> projects/dms/src/styles/vendor/vendor.css && echo @import \"./tailwind.generated.css\"; > projects/dms/src/styles/bundle.css && echo @import \"./vendor/vendor.css\"; >> projects/dms/src/styles/bundle.css && echo @import \"./global.css\"; >> projects/dms/src/styles/bundle.css",
    "copy:vendor": "node scripts/copy-vendor-css.js",
    "build:tailwind": "npx tailwindcss -i ./projects/dms/src/styles/tailwind.css -o ./projects/dms/src/styles/tailwind.generated.css",
    "build:bundle": "postcss ./projects/dms/src/styles/bundle.css -o ./projects/dms/bundle.css",
    "build:all": "npm run build:tailwind && npm run copy:vendor && npm run create:bundle-entry && npm run build:bundle && ng build dms && npm run cleanup:css && npm run cleanup:vendor-assets",
    "cleanup:css": "npx rimraf \"./projects/dms/src/styles/tailwind.generated.css\" && npx rimraf \"./projects/dms/src/styles/bundle.css\" && npx rimraf \"./projects/dms/bundle.css\"",
    "cleanup:vendor-assets": "rimraf projects/dms/src/styles/vendor && rimraf projects/dms/vendor",
    "prepare:dev": "node scripts/copy-vendor-css.js",
    "watch:dms": "ng build dms --watch --configuration development",
    "test": "ng test",
    "storybook": "npm run prepare:dev && ng run dms:storybook",
    "build-storybook": "ng run dms:build-storybook",
    "build:demo": "ng build demo"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@digi/arbetsformedlingen": "^28.10.0",
    "@digi/arbetsformedlingen-angular": "^28.10.0",
    "@tailwindcss/cli": "^4.0.14",
    "@tailwindcss/postcss": "^4.0.14",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.0.14",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.3",
    "@angular/cli": "^19.2.3",
    "@angular/compiler-cli": "^19.2.0",
    "@chromatic-com/storybook": "^3.2.6",
    "@compodoc/compodoc": "^1.1.26",
    "@storybook/addon-docs": "^8.6.7",
    "@storybook/addon-essentials": "^8.6.7",
    "@storybook/addon-interactions": "^8.6.7",
    "@storybook/addon-onboarding": "^8.6.7",
    "@storybook/addon-styling-webpack": "^1.0.1",
    "@storybook/addon-themes": "^8.6.11",
    "@storybook/angular": "^8.6.7",
    "@storybook/blocks": "^8.6.7",
    "@storybook/test": "^8.6.7",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.0",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-url": "^10.1.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "storybook": "^8.6.7",
    "typescript": "~5.7.2"
  }
}
